@page
@model ADONetMovie_RazorPages.Pages.Actors.ActorMoviesModel
@{
}
<h4 style="color:red"> List of ActorMovies</h4>
<table class="table">
    <thead>
    <tr>
        <th>Id </th>
        <th>Title </th>
        <th>Year </th>
        <th>Actor Id  </th>
    </tr>
    </thead>
    <tbody>

    @if (Model.ActorMovies != null)
    {
        @foreach (var item in Model.ActorMovies)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Country</td>
                <td>@item.Title </td>
                <td>@item.Year</td>
            </tr>
        }
    }
</table>
